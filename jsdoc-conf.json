{
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "destination": "./build/docs"
  },
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [
      "./src",
      "./README.md"
    ],
    "exclude": [
      "node_modules",
      "build",
      "coverage"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\|)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
